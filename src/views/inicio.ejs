<!DOCTYPE html>
<html lang="es" dir="ltr">
  <%-include("partials/header")%>
  <body>
    <%-include("partials/nav")%>
    <div class="container p-4">
      <div class="row">
        <div class="col-xl-6 mx-auto my-4">
          <div class="card">
            <div class="card-header bg-success">
              <h2 class="text-white text-center">Registrar un vehiculo</h2>
            </div>
            <div class="card-body bg-success">
              <div class="col-md-8 mx-auto">
                <img class="card-img-top" width="100%" src="img/icons/registrar.png" alt="Registrar">
                <div class="card-img-overlay">
                  <a class="card-link stretched-link" href="/registrar-vehiculo"></a>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="col-xl-6 mx-auto my-4">
          <div class="card">
            <div class="card-header bg-success">
              <h2 class="text-white text-center">Buscar un conductor</h2>
            </div>
            <div class="card-body bg-success">
              <div class="col-md-8 mx-auto">
                <img class="card-img-top" width="100%" src="img/icons/buscar.png" alt="Registrar">
                <div class="card-img-overlay">
                  <a class="card-link stretched-link" href="/buscar-vehiculo"></a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
